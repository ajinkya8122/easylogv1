<% include partials/header.ejs %>


<div class="container">


  <table class="table table-dark">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Desc</th>
        <th scope="col">Incline Bench</th>
        <th scope="col">Bench Pressk</th>
        <th scope="col">Created</th>
      </tr>
    </thead>
    <tbody>
      <% logs.forEach(function(log, i) { %>
      <tr>
        <th scope="row"><%= i+1 %><span><a class="btn btn-secondary btn-sm activ" href="/logs/<%=log._id%>">View</a></span></th>
        
        <td><%= log.desc %></td>
        <td><%= log.exercise.chest.incBenchPress %></td>
        <td><%= log.exercise.chest.bbBenchPress %></td>
        
        <% var date = new Date(log.date); %>
        <% var d = date.getDate(); %>
        <% var m = date.getMonth(); %>
        <% var y = date.getFullYear(); %>
        <% var hr = date.getHours(); %>
        <% var min = date.getMinutes(); %>
        <td><%= d %>-<%= m+1 %>-<%= y %> at (<%=hr-5%>:<%=min%>)</td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
  <a href="/logs/new">Log new entry</a>
  
</div>

<% include partials/footer.ejs %>